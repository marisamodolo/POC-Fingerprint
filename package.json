{
  "name": "fingerprint-capacitor",
  "version": "1.0.0",
  "main": "src/index.html",
  "license": "MIT",
  "scripts": {
    "android": "npx cap open android",
    "ios": "npx cap open ios",
    "build": "rm -rf dist && NODE_ENV=production parcel build src/index.html --no-source-maps && npx cap copy",
    "dev": "npx parcel src/index.html -p 3000 --open",
    "setup": "yarn install && npx cap update",
    "sync": "npx cap sync"
  },
  "dependencies": {
    "@capacitor/android": "2.4.1",
    "@capacitor/cli": "^2.4.1",
    "@capacitor/core": "^2.4.1",
    "@capacitor/ios": "2.4.1",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/fingerprint-aio": "^5.30.0",
    "cordova-plugin-fingerprint-aio": "^4.0.1",
    "es6-promise-plugin": "^4.2.2",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "^0.63.4",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.4",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.2"
  }
}
